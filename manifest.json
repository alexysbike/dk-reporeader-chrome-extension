{
  "manifest_version": 2,
  "name": "Dk Repo Reader",
  "description": "Quick Open Github Repos in DKRR project",
  "version": "1",
  "author": "Alexys Gonzalez (DK)",
  "browser_action": {
    "default_popup": "default-popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": [
    "tabs"
  ]
}
